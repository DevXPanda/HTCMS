{
    "rewrites": [
        {
            "source": "/((?!.*\\.(png|jpg|jpeg|gif|svg|ico|webp|css|js|woff|woff2|ttf|eot|json|xml|txt|pdf|map)$).*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|webp|css|js|woff|woff2|ttf|eot|json|xml|txt|pdf|map))",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}